{
  "name": "lucki",
  "productName": "Lucki Media",
  "version": "0.2.5",
  "description": "A media player",
  "license": "MIT",
  "repository": "Caaz/lucki",
  "author": {
    "name": "Daniel Caaz",
    "email": "daniel@caaz.me",
    "url": "caaz.me"
  },
  "scripts": {
    "test": "xo",
    "start": "electron .",
    "build": "electron-packager . --out=dist --asar --overwrite --all"
  },
  "main": "main.js",
  "files": [
    "app"
  ],
  "keywords": [
    "music",
    "audio",
    "media",
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "electron-debug": "^1.0.0",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.1",
    "id3-parser": "^1.0.3",
    "jquery": "^3.1.1",
    "less": "^2.7.1",
    "mkdirp": "^0.5.1",
    "pug": "^2.0.0-beta6",
    "sprintf-js": "^1.0.3"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron": "^1.3.3",
    "electron-packager": "^8.0.0",
    "require-rebuild": "^1.2.8",
    "xo": "^0.16.0"
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "esnext": true,
    "rules": {
      "max-statements-per-line": 1,
      "guard-for-in": 1,
      "keyword-spacing": 0,
      "no-fallthrough": 0,
      "brace-style": 0,
      "curly": 0
    },
    "envs": [
      "node",
      "browser"
    ]
  }
}
